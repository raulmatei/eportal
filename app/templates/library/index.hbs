{{!-- library content stub --}}
<div class="page-header">
  <h1>Books</h1>
</div>

<ul class="book-list">
  {{#each book in content}}
    <li>
      
    <h3>{{book.title}}<em>({{book.year}})</em> - {{book.author}}</h3> - {{book.book_type}} - {{book.user_id}} - {{book.booked_at}} / {{book.return_at}}

    </li>
  {{/each}}
</ul>

